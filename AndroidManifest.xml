<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mlaskows.quiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mlaskows.quiz.FullscreenActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mlaskows.quiz.ExerciseActivity"
            android:label="@string/title_activity_question"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.mlaskows.quiz.LevelsActivity"
            android:label="@string/title_activity_levels" 
            android:theme="@style/AppTheme" >
        </activity>
    </application>

</manifest>
